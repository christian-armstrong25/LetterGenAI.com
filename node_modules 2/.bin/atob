#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/christianarmstrong/Desktop/CS/side-projects/LetterGen/node_modules/atob/bin/node_modules:/Users/christianarmstrong/Desktop/CS/side-projects/LetterGen/node_modules/atob/node_modules:/Users/christianarmstrong/Desktop/CS/side-projects/LetterGen/node_modules:/Users/christianarmstrong/Desktop/CS/side-projects/node_modules:/Users/christianarmstrong/Desktop/CS/node_modules:/Users/christianarmstrong/Desktop/node_modules:/Users/christianarmstrong/node_modules:/Users/node_modules:/node_modules:/Users/christianarmstrong/Desktop/CS/side-projects/LetterGen/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/christianarmstrong/Desktop/CS/side-projects/LetterGen/node_modules/atob/bin/node_modules:/Users/christianarmstrong/Desktop/CS/side-projects/LetterGen/node_modules/atob/node_modules:/Users/christianarmstrong/Desktop/CS/side-projects/LetterGen/node_modules:/Users/christianarmstrong/Desktop/CS/side-projects/node_modules:/Users/christianarmstrong/Desktop/CS/node_modules:/Users/christianarmstrong/Desktop/node_modules:/Users/christianarmstrong/node_modules:/Users/node_modules:/node_modules:/Users/christianarmstrong/Desktop/CS/side-projects/LetterGen/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../atob/bin/atob.js" "$@"
else
  exec node  "$basedir/../atob/bin/atob.js" "$@"
fi
