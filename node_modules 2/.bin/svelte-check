#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/christianarmstrong/Desktop/CS/side-projects/LetterGen/node_modules/.pnpm/svelte-check@3.4.3_@babel+core@7.26.0_postcss@8.4.24_svelte@3.59.1/node_modules/svelte-check/bin/node_modules:/Users/christianarmstrong/Desktop/CS/side-projects/LetterGen/node_modules/.pnpm/svelte-check@3.4.3_@babel+core@7.26.0_postcss@8.4.24_svelte@3.59.1/node_modules/svelte-check/node_modules:/Users/christianarmstrong/Desktop/CS/side-projects/LetterGen/node_modules/.pnpm/svelte-check@3.4.3_@babel+core@7.26.0_postcss@8.4.24_svelte@3.59.1/node_modules:/Users/christianarmstrong/Desktop/CS/side-projects/LetterGen/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/christianarmstrong/Desktop/CS/side-projects/LetterGen/node_modules/.pnpm/svelte-check@3.4.3_@babel+core@7.26.0_postcss@8.4.24_svelte@3.59.1/node_modules/svelte-check/bin/node_modules:/Users/christianarmstrong/Desktop/CS/side-projects/LetterGen/node_modules/.pnpm/svelte-check@3.4.3_@babel+core@7.26.0_postcss@8.4.24_svelte@3.59.1/node_modules/svelte-check/node_modules:/Users/christianarmstrong/Desktop/CS/side-projects/LetterGen/node_modules/.pnpm/svelte-check@3.4.3_@babel+core@7.26.0_postcss@8.4.24_svelte@3.59.1/node_modules:/Users/christianarmstrong/Desktop/CS/side-projects/LetterGen/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../svelte-check/bin/svelte-check" "$@"
else
  exec node  "$basedir/../svelte-check/bin/svelte-check" "$@"
fi
